; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Area2D",
"class": "Bullet",
"language": "GDScript",
"path": "res://Scripts/Bullet.gd"
}, {
"base": "Area2D",
"class": "Card",
"language": "GDScript",
"path": "res://Scripts/Card.gd"
}, {
"base": "KinematicBody2D",
"class": "Controller",
"language": "GDScript",
"path": "res://Scripts/Controller.gd"
}, {
"base": "KinematicBody2D",
"class": "Enemy",
"language": "GDScript",
"path": "res://Scripts/Enemy.gd"
}, {
"base": "KinematicBody2D",
"class": "Tower",
"language": "GDScript",
"path": "res://Scripts/Tower.gd"
} ]
_global_script_class_icons={
"Bullet": "",
"Card": "",
"Controller": "",
"Enemy": "",
"Tower": ""
}

[application]

config/name="A_TD_Project"
run/main_scene="res://Levels/World.tscn"
config/icon="res://Sprites/icon.png"

[autoload]

Data="*res://Scripts/Data.gd"

[display]

window/size/width=720
window/size/height=480
window/size/test_width=720
window/size/test_height=480
window/stretch/mode="2d"
window/stretch/aspect="keep"

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[rendering]

environment/default_environment="res://Assets/default_env.tres"
